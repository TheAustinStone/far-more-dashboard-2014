<script type='text/javascript'>
$(function() {
Dashing.gridsterLayout('[{"col":4,"row":3,"size_x":1,"size_y":1},{"col":1,"row":1,"size_x":1,"size_y":2},{"col":4,"row":1,"size_x":1,"size_y":2},{"col":2,"row":1,"size_x":2,"size_y":2},{"col":1,"row":3,"size_x":2,"size_y":1}]')
  Dashing.widget_base_dimensions = [370, 340]
  Dashing.numColumns = 5
});
</script>

<% content_for :title do %>1080p dashboard<% end %>

<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="Image" data-image="/logo.png"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
      <div data-id="farmore-campus-leaderboard" data-view="List"
        data-unordered="true"
        data-title="Campuses"
        data-moreinfo="Sign ups per campus"
      ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
      <div data-id="farmore-next-steps" data-view="List"
        data-unordered="true"
        data-title="Next steps"
        data-moreinfo=""
      ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="2">
      <div data-id="farmore-weekly" data-view="Graph"
        data-title="Sign ups"
        style="background-color:#47bbb3;"
      ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="twitter_mentions" data-view="Comments"
        style="background-color:#ff9618;"
        data-moreinfo="Tweets with #farmore2014"
      ></div>
      <i class="icon-twitter icon-background"></i>
    </li>
  </ul>
</div>
