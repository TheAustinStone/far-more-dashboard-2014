<% content_for :title do %>1080p dashboard<% end %>

<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="Image" data-image="/logo.png" data-text="Far More 2014"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
      <div data-id="farmore-campus-leaderboard" data-view="List"
        data-unordered="true"
        data-title="Campuses"
      ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
      <div data-id="farmore-next-step" data-view="List"
        data-unordered="true"
        data-title="NEXT STEP"
        data-moreinfo=""
      ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
      <div data-id="farmore-serve" data-view="List"
        data-unordered="true"
        data-title="SERVE"
        data-moreinfo=""
      ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="2">
      <div data-id="farmore-commit-own" data-view="List"
        data-unordered="true"
        data-title="COMMIT/OWN"
        data-moreinfo=""
      ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="farmore-weekly" data-view="Graph"
        data-title="SIGN UPS"
      ></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="twitter_mentions" data-view="Comments"
        data-moreinfo="Tweets with #farmore2014"
      ></div>
      <i class="icon-twitter icon-background"></i>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="real-time-feed" data-view="Updatelist"
        data-unordered="true"
      ></div>
    </li>
  </ul>
</div>
